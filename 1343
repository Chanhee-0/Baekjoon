#include<iostream>
#include<string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int n = 0;
	string s, r, a = "AAAA", b = "BB", c = ".";

	cin >> s;

	for (int i = 0; i < s.size(); i++) {
		if (s[i] == 'X') {
			n++;
			if (i == s.size() - 1) {
				if (n % 4 == 0) {
					for (int j = 0; j < n / 4; j++) {
						r += a;
					}
				}
				else if (n % 4 == 2) {
					for (int j = 0; j < n / 4; j++) {
						r += a;
					}
					r += b;
				}
				else {
					cout << "-1";
					return 0;
				}
			}
		}
		else if (s[i] == '.') {
			if (n == 0) {
				r += c;
				continue;
			}
			if (n % 4 == 0) {
				for (int j = 0; j < n / 4; j++) {
					r += a;
				}
			}
			else if (n % 4 == 2) {
				for (int j = 0; j < n / 4; j++) {
					r += a;
				}
				r += b;
			}
			else {
				cout << "-1";
				return 0;
			}
			r += c;
			n = 0;
		}
	}

	cout << r;

	return 0;
}
/*
1343 폴리오미노
*/

#include<iostream>
#include<string>

using namespace std;

int n = 0;
string s, r, a = "AAAA", b = "BB", c = ".";

int func() {
	if (n % 4 == 0) {
		for (int j = 0; j < n / 4; j++) {
			r += a;
		}
		return 0;
	}
	else if (n % 4 == 2) {
		for (int j = 0; j < n / 4; j++) {
			r += a;
		}
		r += b;
		return 0;
	}
	else {
		cout << "-1";
		return 1;
	}
}

int main() {
	ios::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	cin >> s;

	for (int i = 0; i < s.size(); i++) {
		if (s[i] == 'X') {
			n++;
			if (i == s.size() - 1) {
				if (func())
					return 0;
			}
		}
		else if (s[i] == '.') {
			if (n == 0) {
				r += c;
				continue;
			}
			if (func())
				return 0;
			r += c;
			n = 0;
		}
	}

	cout << r;

	return 0;
}
